{
  "default_locale": "en",
  "manifest_version": 3,
  "name": "__MSG_extName__",
  "version": "2.0",
  "action": {
    "default_title": "__MSG_extensionIconTooltip__",
    "default_popup": "popup.html"
  },
  "description": "__MSG_extensionDescription__",
"permissions": [
  "bookmarks",
  "contextMenus",
  "tabs"
],
"host_permissions": [
  "<all_urls>"
],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src chrome://favicon; child-src chrome://bookmarks"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "logo.png",
    "128": "logo.png"
  }
}