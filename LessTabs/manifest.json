{
  "default_locale":"en",
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0",
  "browser_action": {
      "default_title": "__MSG_extensionIconTooltip__",
      "default_popup":"popup.html"
  },
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "bookmarks","tabs","chrome://favicon/*"
